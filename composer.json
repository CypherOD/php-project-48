{
    "name": "hexlet/code",
    "type": "library",
    "license": "MIT",
    "bin": [
        "bin/gendiff"
    ],
    "autoload": {
        "psr-4": {
            "Differ\\": "src/"
        },
        "files": [
            "src/Differ.php",
            "src/FileReader.php",
            "src/Parsers.php",
            "src/Formatters.php",
            "src/Formatters/Helpers.php",
            "src/Formatters/Json.php",
            "src/Formatters/Plain.php",
            "src/Formatters/Stylish.php"
        ]
    },
    "authors": [
        {
            "name": "gennadiy-dev",
            "email": "gennadiy.dev@yandex.ru"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^12.1",
        "symfony/var-dumper": "^7.2",
        "squizlabs/php_codesniffer": "^3.12"
    },
    "require": {
        "php": ">=8.1",
        "docopt/docopt": "^1.0",
        "symfony/yaml": "^7.2",
        "illuminate/collections": "^12.10"
    }
}
